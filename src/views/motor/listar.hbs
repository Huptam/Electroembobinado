<div class="row">
  <div class="col-12">
    <div class="card">
      <div class="card-header">
        <h3 class="card-title">Listado de Motores<div class="float-right"> <a href="motores/agregar-motor">
        <button class="btn btn-success">AÃ±adir Motor</button></a> </div>
        </h3>
        <div class="card-tools">
          <div class="input-group input-group-sm" style="width: 300px;">
            <input type="text" name="table_search" class="form-control float-right" placeholder="Search">
            <div class="input-group-append">
              <button type="submit" class="btn btn-secondary">
                <i class="fa fa-search"></i>
              </button>
            </div>
          </div>
        </div>
      </div>
      <div class="card-body table-responsive p-0">
        <table class="table table-hover text-nowrap">
          <thead>
            <tr>
              <th>Propietario</th>
              <th>Tipo de Motor</th>
              <th>Marca</th>
              <th>Modelo</th>
              <th>PH</th>
              <th>HP</th>
              <th>Ranueras</th>
              <th>Voltaje</th>
              <th>Fecha de Ingreso</th>
              <th>Descripci&oacute;n</th>
              <th>Acciones</th>
            </tr>
          </thead>
          <tbody>
            {{#each motor}}
            <tr>
              <td>{{id_cliente}}</td>
              <td>{{Nombre}}</td>
              <td>{{Marca}}</td>
              <td>{{Modelo}}</td>
              <td>{{Ph}}</td>
              <td>{{Hp}}</td>
              <td>{{Ranuras}}</td>
              <td>{{Voltaje}}</td>
              <td>{{Fecha_ingreso}}</td>
              <td>{{Descripcion}}</td>
              <td>
                <a href=""><button class="btn btn-info">Editar</button></a>
                <a href=""><button class="btn btn-danger">Eliminar</button></a>
              </td>
            </tr>
            {{/each}}
        </table>
      </div>
    </div>
  </div>
</div>